<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Vehicles Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<!-- Acceleration Stats -->
				<li Class="PatchOperationAdd">
					<xpath>Defs/Vehicles.VehicleDef[defName="VCE_CannonVehicle"]/vehicleStats</xpath>
					<value>
						<AccelerationRate>0.01</AccelerationRate>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>